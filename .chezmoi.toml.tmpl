{{- $git_email := promptStringOnce . "git_email" "Git email" -}}
{{- $git_name := promptStringOnce . "git_name" "Git name" -}}
{{- $git_signingkey := promptStringOnce . "git_signingkey" "Git signingkey" -}}
{{- $emacs_name := promptStringOnce . "emacs_name" "Emacs name" -}}
{{- $emacs_email := promptStringOnce . "emacs_email" "Emacs email" -}}
{{- $todoist_api_token := promptStringOnce . "todoist_api_token" "Todoist API token" -}}
{{- $roborev_default_agent := default "codex" (promptString "roborev_default_agent" "RoboRev default agent") -}}
{{- $roborev_design_agent := promptString "roborev_design_agent" "RoboRev design agent (optional)" -}}
{{- $roborev_fix_agent := promptString "roborev_fix_agent" "RoboRev fix agent (optional)" -}}
{{- $roborev_review_agent := promptString "roborev_review_agent" "RoboRev review agent (optional)" -}}

[data]
    git_email = {{ $git_email | quote }}
    git_name = {{ $git_name | quote }}
    git_signingkey = {{ $git_signingkey | quote }}
    emacs_name = {{ $emacs_name | quote }}
    emacs_email = {{ $emacs_email | quote }}
    todoist_api_token = {{ $todoist_api_token | quote }}
    roborev_default_agent = {{ $roborev_default_agent | quote }}
    roborev_design_agent = {{ $roborev_design_agent | quote }}
    roborev_fix_agent = {{ $roborev_fix_agent | quote }}
    roborev_review_agent = {{ $roborev_review_agent | quote }}
